---
import BlogGridItem from "./BlogGridItem.astro";
const { posts } = Astro.props;
export const prerender = true;
---

<div class="grid grid-cols-4 gap-6">
  {
    posts.data && posts.data.length > 0
      ? posts.data.map((post) => <BlogGridItem post={post} />)
      : "No posts founds"
  }
</div>